{
  "name": "rumors-site",
  "version": "0.0.1",
  "description": "Rumors list / creation UI, with server-side rendering",
  "main": "index.js",
  "author": "MrOrz",
  "license": "MIT",
  "scripts": {
    "dev": "babel-node server",
    "lint": "eslint --fix .",
    "build": "docker build -t mrorz/rumors-site:latest .",
    "build:next": "next build",
    "build:server": "babel server.js -o server.dist.js",
    "init:autotrack": "autotrack -o static/$npm_package_config_autotrackFileName -p eventTracker,outboundLinkTracker,urlChangeTracker",
    "start": "NODE_ENV=production node server.dist.js"
  },
  "config": {
    "autotrackFileName": "autotrack.custom.js"
  },
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-css-import-to-string": "^0.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babel-runtime": "^6.25.0",
    "glob": "^7.1.2",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.3.0",
    "koa-router": "^7.2.1",
    "next": "^3.0.1",
    "normalize.css": "^7.0.0",
    "nprogress": "^0.2.0",
    "postcss-color-function": "^4.0.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-loader": "^2.0.6",
    "postcss-nesting": "^2.3.1",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-radio-group": "^3.0.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-duck": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "styled-jsx-postcss": "^0.2.0"
  },
  "devDependencies": {
    "autotrack": "^2.4.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "prettier": "^1.4.4",
    "redux-devtools-extension": "^2.13.2"
  }
}
